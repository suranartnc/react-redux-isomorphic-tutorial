{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "rules": {
    "linebreak-style": [0],
    "import/no-unresolved": [0],
    "react/jsx-filename-extension": [0],
    "import/no-extraneous-dependencies": [1],
    "global-require": [1]
  }
}